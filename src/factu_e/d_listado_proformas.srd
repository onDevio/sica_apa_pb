         (select cf.descripcion from csi_cobros_multiples, csi_formas_de_pago cf , csi_cobros where csi_cobros.id_cobro_multiple = csi_cobros_multiples.id_cobro_multiple and cf.tipo_pago = csi_cobros_multiples.forma_pago and csi_cobros.id_factura = csi_facturas_emitidas.id_factura) as fp_cm  
    FROM csi_facturas_emitidas,   
         csi_formas_de_pago  
   WHERE ( csi_facturas_emitidas.formadepago *= csi_formas_de_pago.tipo_pago)   AND  

         csi_facturas_emitidas.centro,  
         convert(char(1), 'N') as contabilizar_cobro, 
         'N' as con_cobros_multiples,
			csi_cobros.id_pago, 
			csi_cobros.id_cobro_multiple, 
			csi_cobros.forma_pago,
			csi_cobros.banco,
			csi_cobros.contabilizado,
			csi_cobros.f_contabilizado,
			csi_cobros.pagado,
			csi_cobros.importe,
			csi_cobros.f_pago, 
			csi_cobros.n_remesa,
			csi_facturas_emitidas.empresa,
			csi_cobros.n_plazo
    FROM csi_facturas_emitidas,
			csi_cobros
   WHERE ( csi_facturas_emitidas.id_factura = csi_cobros.id_factura ) and
         (
				(( csi_facturas_emitidas.contabilizada = 'S' ) AND ( csi_cobros.contabilizado <> 'S' )) or
				(( csi_facturas_emitidas.contabilizada = 'N' ) AND ( csi_cobros.n_remesa <> '' and csi_cobros.n_remesa is not null ) and ( csi_cobros.contabilizado <> 'S' ) and csi_cobros.forma_pago<>'PP')
			) and
			( csi_cobros.pagado = 'S' ) and
	( csi_cobros.importe <> 0) and
         ( csi_cobros.f_pago >= :df ) AND  
         ( csi_cobros.f_pago < :hf ) AND  
         ( csi_facturas_emitidas.centro like :centro ) AND  
         ( ( csi_facturas_emitidas.id_fase is NULL ) OR  
         ( csi_facturas_emitidas.id_fase = '' ) )    AND
			( csi_facturas_emitidas.n_fact like :serie )
